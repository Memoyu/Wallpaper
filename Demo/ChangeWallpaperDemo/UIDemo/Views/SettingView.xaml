        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <cc:Navigation ContentTitle ="标题栏！！！" GoBackCommand="{Binding GoBackCommand}" />
